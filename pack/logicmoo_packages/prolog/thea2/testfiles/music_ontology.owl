        </owl:inverseOf>
    <rdfs:domain rdf:resource="http://purl.org/ontology/mo/MusicalManifestation"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/mo/"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/publishing_location"
rdfs:label="publishingLocation" vs:term_status="stable" mo:level="1">
