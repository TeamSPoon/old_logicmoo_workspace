	<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/mo/"/>
</owl:ObjectProperty>


<owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/publisher" rdfs:label="publisher" vs:term_status="unstable" mo:level="1">
