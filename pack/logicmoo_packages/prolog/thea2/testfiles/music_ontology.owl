</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/supporting_musician" rdfs:label="supporting_musician" vs:term_status="unstable" mo:level="1">
	<rdfs:comment>Used to relate an artist doing long-time instrumental or vocal support for another artist.</rdfs:comment>
	<rdfs:domain rdf:resource="http://purl.org/ontology/mo/MusicArtist"/>
	<rdfs:range rdf:resource="http://purl.org/ontology/mo/MusicArtist"/>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/mo/"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/collaborated_with" rdfs:label="collaborated_with" vs:term_status="unstable" mo:level="1">
