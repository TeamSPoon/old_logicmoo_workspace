	<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/isPrimaryTopicOf"/>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/mo/"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/musicbrainz"
rdfs:label="musicbrainz" vs:term_status="stable" mo:level="1">
