	<rdfs:range rdf:resource="http://purl.org/ontology/mo/Signal"/>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/mo/"/>
</owl:ObjectProperty>


<owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/sampler" rdfs:label="sampler" vs:term_status="unstable" mo:level="1">
	<rdfs:comment>Used to relate the manifestation of a musical work to an artist who sampled it.</rdfs:comment>
