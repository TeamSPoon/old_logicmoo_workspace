<owl:Class rdf:about="&ns1;WhiteLoire">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:allValuesFrom>
        <rdf:Description>
          <owl:oneOf>
            <rdf:Description>
              <rdf:first rdf:resource="&ns1;CheninBlancGrape"/>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first rdf:resource="&ns1;PinotBlancGrape"/>
                  <rdf:rest>
                    <rdf:Description>
                      <rdf:first rdf:resource="&ns1;SauvignonBlancGrape"/>
                      <rdf:rest rdf:resource="&rdf;nil"/>
                    </rdf:Description>
                  </rdf:rest>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </owl:oneOf>
        </rdf:Description>
      </owl:allValuesFrom>
      <owl:onProperty rdf:resource="&ns1;madeFromGrape"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <owl:equivalentClass>
    <rdf:Description>
      <owl:intersectionOf>
        <rdf:Description>
          <rdf:first rdf:resource="&ns1;Loire"/>
          <rdf:rest>
            <rdf:Description>
              <rdf:first rdf:resource="&ns1;WhiteWine"/>
              <rdf:rest rdf:resource="&rdf;nil"/>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </owl:intersectionOf>
    </rdf:Description>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="&ns1;Loire"/>

