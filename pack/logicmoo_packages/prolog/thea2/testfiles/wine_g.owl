<owl:Class rdf:about="&ns1;WineDescriptor"
    rdfs:comment="Made WineDescriptor unionType of tastes and color">
  <owl:equivalentClass>
    <rdf:Description>
      <owl:unionOf>
        <rdf:Description>
          <rdf:first rdf:resource="&ns1;WineTaste"/>
          <rdf:rest>
            <rdf:Description>
              <rdf:first rdf:resource="&ns1;WineColor"/>
              <rdf:rest rdf:resource="&rdf;nil"/>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </owl:unionOf>
    </rdf:Description>
  </owl:equivalentClass>
</owl:Class>

