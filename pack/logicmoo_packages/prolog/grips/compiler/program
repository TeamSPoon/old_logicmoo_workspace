program p;

label
    99, 100;

const
    five = 5;

var
    v : integer;
    w : integer;

begin
    write('Hello.');
    v := 1;
    w := 1;
99:
        if v=five then goto 100;
        v := v + 1;
        w := w * v;
        goto 99;
100:
    write('v = ');
    write(v);
    write('v! = ');
    write(w)
end.
