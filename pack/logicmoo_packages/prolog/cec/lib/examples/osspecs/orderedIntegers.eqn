module orderedIntegers using intbase + totalOrder([(elem,int)],[]).

var [p : posint, k : nat, m : nat] .

(0 =< p) = true .
(- m =< p) = true .
(m =< - p) = false .
(s(m) =< s(k)) = (m =< k) .
(- k =< - m) = (m =< k).


% inductive theorem

(s(m) > s(k)) = (m > k).
(-k > -m) = (m > k).

