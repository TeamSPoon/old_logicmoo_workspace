%default q(20,X).
fact not q(X,Y) <- q(X,Z), lt(Z,Y).
fact not q(X,Y) <- q(W,Z) , (pplus(X,D,W),(pplus(Y,D,Z); pplus(Z,D,Y));
   pplus(W,D,X),pplus(Y,D,Z)).
fact lt(X,Y) <- pplus(X,Z,Y).
