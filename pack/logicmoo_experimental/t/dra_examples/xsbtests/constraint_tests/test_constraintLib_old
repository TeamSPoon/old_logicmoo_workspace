no(b,a)
no(f(A),f(a,B))
no(f(A,g(a)),f(B,g(b)))
test(1 - 2,nonvar)
test(3 - 2,ground)
test(4 - 2,?=)
test(6 - 2,?=)
testwhen(1,nonvar)
testwhen(2,ground)
testwhen(3,ground)
testwhen(4,?=)
testwhen(5,?=)
testwhen(6,?=)
yes(A,A,[])
yes(A,B,[A = B])
yes(A,a(B),[A = a(B)])
yes(A,a,[A = a])
yes(a,A,[A = a])
yes(f(A),f(A),[])
yes(f(A),f(B),[A = B])
yes(f(A),f(a),[A = a])
yes(f(A,g(B)),f(C,g(b)),[A = C,B = b])
