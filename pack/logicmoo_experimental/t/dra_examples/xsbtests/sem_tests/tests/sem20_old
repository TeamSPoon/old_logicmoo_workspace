'(ran(rita) & laughed(rita))'
'(ran(rita) & laughed(rita))'
'(ran(rita) \\/ laughed(rita))'
'(ran(rita) \\/ laughed(rita))'
'laughed(rita)'
'member(rita, ({x : ran(x)} /\\ {y : laughed(y)}))'
'member(rita, ({x : ran(x)} \\/ {y : laughed(y)}))'
'member(rita, {x : (member(x, {y : ran(y)}) & member(x, {z : laughed(z)}))})'
'member(rita, {x : (member(x, {y : ran(y)}) \\/ member(x, {z : laughed(z)}))})'
'member(rita, {x : laughed(x)})'
'member(rita, {x : ran(x)})'
'ran(rita)'
