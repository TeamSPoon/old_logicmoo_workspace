.Asrg_telvcr_svenska TelVcrTop
TelVcrTop [ TelVcrAtom
	    ( ?(TelVcrAtom -pau-) TelVcrQuestion )
	    ( ?(TelVcrAtom -pau-) RequestSeq )
	    ( ?(TelVcrAtom -pau-) ShortAns ?RequestSeq )
	     hej 
            (hej då) 
             sluta 
	  ]

TelVcrAtom [ förlåt okej (vet inte) ja nej ]
 
;QUESTIONS
TelVcrQuestion [  
		;ask(usage)
		hjälp 

		 ;ask(X^channels)
		(vilka kanaler finns det)
		(vilka kanaler) 
		(vad finns det för kanaler)

		;ask(X^rec_jobs(X)) ;bara inspelningar är obligatoriskt
		( ?[(vilka ?är) visa säg lista ] 
			?mina ?planerade inspelningar ?(har jag))
		
                 ]
;short_answers 
ShortAns [ Channel Number_0_to_109 Time Day ]
;requests 
 
RequestSeq [ [toppnivå (börja om) ] 
	     ( ?[ toppnivå (börja om) ] 
		[ TelVcrAddRecJob
	     TelVcrDeleteRecJob ] )
	     ] 
    
TelVcrAddRecJob  (?[ ( [ lägga lägg] till ?[(?en inspelning)(?ett program) ] )
		      	( spela in )]  
			?TelVcrAddRecJobParams )
 
TelVcrDeleteRecJob ( ?([(ta bort) radera] ?[(?ett program) (?en inspelning)] )
			?Number_0_to_109 ) ;
 
