macro(day_of_week_noun(Words, Sem),
      @noun(Words, [sem=Sem, sem_n_type=date, @takes_no_pps, takes_det_type=(def\/indef\/quant\/ordinal\/null)])).

@day_of_week_noun([monday, mondays], @noun_sem(day, monday)).
