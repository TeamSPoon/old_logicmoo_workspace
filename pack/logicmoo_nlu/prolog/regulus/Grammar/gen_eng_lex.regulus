@day_of_week_noun([wednesday, wednesdays], @noun_sem(day, wednesday)).
@day_of_week_noun([thursday, thursdays], @noun_sem(day, thursday)).
@day_of_week_noun([friday, fridays], @noun_sem(day, friday)).
@day_of_week_noun([saturday, saturdays], @noun_sem(day, saturday)).
@day_of_week_noun([sunday, sundays], @noun_sem(day, sunday)).

% As in "three times a week"
n:[sem= @noun_sem(frequency, time), agr=3/\plur, sem_n_type=timeunit, takes_frequency_pp=y, takes_det_type=(numeric\/quant)] --> times.

macro(frequency_np(Sem, Surface),
      (np:[sem=[[frequency, Sem]], agr=3, wh=n, sem_n_type=timeunit, takes_post_mods=y, takes_frequency_pp=y, gapsin=GIn, gapsout=GIn, pronoun=n, syn_type=np_with_noun] --> Surface)).

@frequency_np(once, once).
@frequency_np(at_least_once, (at, least, once)).
@frequency_np(more_than_once, (more, than, once)).
@frequency_np(twice, twice).
@frequency_np(at_least_twice, (at, least, twice)).
@frequency_np(more_than_twice, (more, than, twice)).

