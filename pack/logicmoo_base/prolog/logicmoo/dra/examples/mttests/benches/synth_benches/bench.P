mt_bigbench(Call,Type,Its,M):-
	walltime(T1),
	n_par_execute(bigbench(Its,Call),M),
	walltime(T2), T is T2 - T1,
	writeq(datum(Type,M,T)),writeln('.'),nl,
	assert(datum(Type,M,T)).


bigbench(Call,Type,Its):- 
	cputime(T),bigbench(Its,Call),cputime(T1),Tot is T1 - T,
	writeq(datum(Type,Tot)),writeln('.'),nl,
	assert(datum(Type,Tot)).

bigbench(0,_):-!.
bigbench(N,Call):-
%	writeln(calling),
	(tcall(Call),fail; true),
	abolish_tables,
	N1 is N - 1,
	bigbench(N1,Call).

