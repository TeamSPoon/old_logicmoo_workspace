smallbench(Call,Type):-
	writeln('--------------------------------------------------------'),
	writeln(Type),
	cputime(T),call(Call),cputime(T1),Tot is T1 - T, writeln(Tot),
	writeq(datum(Type,Tot)),writeln('.'),
	assert(datum(Type,Tot)).

smallread(Call1,Call2,Type):-
	writeln('--------------------------------------------------------'),
	writeln(Type),
	(call(Call1),fail ; true),
	cputime(T),call(Call2),cputime(T1),abolish_table_info,
	Tot is T1 - T, writeln(Tot),
	writeq(datum(Type,Tot)),writeln('.'),
	assert(datum(Type,Tot)).

bigbench(Call,Type,Its):- 
	writeln('--------------------------------------------------------'),
	writeln(Type),
	cputime(T),bigbench(Its,Call),cputime(T1),Tot is T1 - T, writeln(Tot),
	writeq(datum(Type,Tot)),writeln('.'),
	assert(datum(Type,Tot)).

bigbench(0,_):-!.
bigbench(N,Call):-
	(call(Call),fail; true),
	abolish_all_tables,
	N1 is N - 1,
	bigbench(N1,Call).

bigread(Call,Type,Its):- 
	writeln('--------------------------------------------------------'),
	writeln(Type),
	(call(Call),fail ; true),
	cputime(T),zero_out_profile_table,
	bigread(Its,Call),cputime(T1),Tot is T1 - T, 
	writeln(Tot),nl.
bigread(0,_):-!.
bigread(N,Call):-
	(call(Call),fail ; true),
	N1 is N - 1,
	bigread(N1,Call).

make_data_file(File):-
	str_cat(File,'_new',Output),
	tell(Output),
	make_data_file_1,
	told.

make_data_file_1:-
	datum(Type,Val),
	Term = datum(Type,Val),
	writeq(Term),write('.'),nl,
	fail.
make_data_file_1.


