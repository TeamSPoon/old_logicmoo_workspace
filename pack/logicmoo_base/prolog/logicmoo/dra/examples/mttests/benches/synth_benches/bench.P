chain(Length,From,To):- 
	From < Length,
	To is From + 1.

cycle(Length,From,To):- 
	From < Length -> To is From + 1
	 ; (From = Length -> To = From).

genchain(Length):- 
	genchain_1(1,Length).

genchain_1(Length,Length):- !.
genchain_1(L,Length):- 
	L1 is L + 1,
	write(move(L,L1)),writeln('.'),
	genchain_1(L1,Length).
	
	

