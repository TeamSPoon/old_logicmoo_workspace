  <xsd:complexType name="DataPropertySynset">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="owl:DataProperty" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:group name="SetOfDataPropertySynsets">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="DataPropertySynset" type="ol:DataPropertySynset" />
    </xsd:sequence>
  </xsd:group>
  <!--<xsd:element name="PublicKB">
    <xsd:complexType>
      <xsd:attribute name="kb" type="xsd:anyURI" use="required" />
      <xsd:attribute name="name" type="xsd:string" use="required"/>
    </xsd:complexType>
  </xsd:element>-->
  <xsd:element name="Description">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="1" maxOccurs="1" name="protocolVersion" type="ol:Version" />
        <xsd:element minOccurs="1" maxOccurs="1" name="reasonerVersion" type="ol:Version" />
        <xsd:group minOccurs="0" ref="ol:Setup" />
        <xsd:element minOccurs="0" maxOccurs="unbounded" name="SupportedExtension">
          <xsd:complexType>
            <xsd:attribute name="identifier" type="xsd:anyURI" use="required" />
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="0" maxOccurs="unbounded" name="PublicKB">
          <xsd:complexType>
            <xsd:attribute name="kb" type="xsd:anyURI" use="required" />
            <xsd:attribute name="name" type="xsd:string" use="required" />
          </xsd:complexType>
        </xsd:element>
        <!-- <xsd:element ref="ol:PublicKB" minOccurs="0" maxOccurs="unbounded"/>-->
      </xsd:sequence>
      <xsd:attribute name="name" type="xsd:string" use="required" />
      <xsd:attribute name="message" type="xsd:string" use="optional" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="KB">
    <xsd:complexType>
      <xsd:complexContent mixed="false">
        <xsd:extension base="ol:KBResponse">
          <xsd:attribute name="kb" type="xsd:anyURI" />
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Settings">
    <xsd:complexType>
      <xsd:complexContent mixed="false">
        <xsd:extension base="ol:KBResponse">
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Setting" type="ol:Setting" />
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="OK" type="ol:KBResponse" />
  <xsd:element name="SetOfAnnotationProperties">
    <xsd:complexType>
      <xsd:complexContent mixed="false">
        <xsd:extension base="ol:KBResponse">
          <xsd:group ref="ol:SetOfAnnotationProperties" />
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SetOfClasses">
    <xsd:complexType>
      <xsd:complexContent mixed="false">
        <xsd:extension base="ol:KBResponse">
          <xsd:group ref="ol:SetOfClasses" />
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SetOfIndividuals">
    <xsd:complexType>
      <xsd:complexContent mixed="false">
