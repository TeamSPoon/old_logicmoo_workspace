  </xsd:complexType>
  <xsd:group name="SetOfClasses">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="owl:Class" />
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="SetOfClasses">
    <xsd:group minOccurs="0" maxOccurs="unbounded" ref="ol:SetOfClasses" />
  </xsd:complexType>
  <xsd:complexType name="ClassSynset">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="owl:Class" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:group name="SetOfClassSynsets">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="ClassSynset" type="ol:ClassSynset" />
    </xsd:sequence>
  </xsd:group>
  <!--<xsd:group name="SetOfSubClassSynsets">
    <xsd:sequence>
      <xsd:element name="ClassSynset" type="ol:ClassSynset" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:group>-->
  <xsd:group name="SetOfIndividuals">
    <xsd:sequence>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="owl:NamedIndividual" />
        <xsd:element ref="owl:AnonymousIndividual" />
      </xsd:choice>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="IndividualSynset">
    <xsd:sequence>
      <xsd:choice minOccurs="1" maxOccurs="unbounded">
        <xsd:element ref="owl:NamedIndividual" />
        <xsd:element ref="owl:AnonymousIndividual" />
      </xsd:choice>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:group name="SetOfIndividualSynsets">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="IndividualSynset" type="ol:IndividualSynset" />
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="SetOfAnnotationProperties">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="owl:AnnotationProperty" />
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="SetOfLiterals">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="owl:Literal" />
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="SetOfDatatypes">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="owl:Datatype" />
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="SetOfObjectProperties">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="owl:ObjectProperty" />
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="ObjectPropertySynset">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="owl:ObjectProperty" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:group name="SetOfObjectPropertySynsets">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="ObjectPropertySynset" type="ol:ObjectPropertySynset" />
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="SetOfDataProperties">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="owl:DataProperty" />
