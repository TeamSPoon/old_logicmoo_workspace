                    <xsd:complexType>
                      <xsd:group ref="ol:SetOfDataPropertySynsets"></xsd:group>
                    </xsd:complexType>
                  </xsd:element>-->
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
