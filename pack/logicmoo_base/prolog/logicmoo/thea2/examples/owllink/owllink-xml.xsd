  <!-- Ask-ClassAsks-->
  <xsd:element name="StringResponse">
    <xsd:complexType>
      <xsd:complexContent mixed="false">
        <xsd:extension base="ol:KBResponse">
          <xsd:attribute name="result" type="xsd:string" />
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
