<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ResponseMessage [
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#"> ]>

<ResponseMessage
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.owllink.org/owllink-xml# http://www.owllink.org/owllink-xml-20091016.xsd"
    xmlns="http://www.owllink.org/owllink-xml#"
    xmlns:owl="http://www.w3.org/2002/07/owl#">
    
    <!-- KB management -->
    <KB kb="KB_1"/>
    <KB kb="KB_2"/>
    
    <!-- tell -->
    <OK/>
    
    <!-- ask -->
    
    <!-- GetAllClasses -->
    <SetOfClasses>
        <owl:Class IRI="F"/>
        <owl:Class IRI="C"/>
        <owl:Class IRI="E"/>
        <owl:Class IRI="B"/>
        <owl:Class IRI="A"/>
    </SetOfClasses>
    
    
    
    <!-- GetEquivalentClasses -->
    <SetOfClasses>
        <owl:Class IRI="E"/>
        
    </SetOfClasses>
    
    <!-- IsClassSubsumedBy -->
    <BooleanResponse result="false"/>
    
    <!-- GetSubClassOf -->
    <SetOfClassSynsets>
        <ClassSynset>
            <owl:Class IRI="&owl;Nothing"/>
            <owl:Class IRI="F"/>
        </ClassSynset>
    </SetOfClassSynsets>
    
    <!--Some tells in another KB -->
    <OK/>
    
    <!-- All datatypes of the KB -->
    <SetOfDatatypes>
        <owl:Datatype IRI="&xsd;integer"/>
    </SetOfDatatypes>
    
    <!-- KB management -->
    <OK/>
    
       
    <!-- One more ask -->
    <KBError errorMessage="KB KB_1 does not exist!"/>
    
    
    <Description name="Racer">
        <protocolVersion major="" minor=""/>
        <reasonerVersion major="" minor=""/>
        <Property key="supportedDatatypes">
            <OneOf type="IRI">
                <Literal>dfasdfasd"</Literal>
            </OneOf>
            
        </Property>
        
    </Description>
    
    

</ResponseMessage>