	<CustomFolders>
		<Folder
			Name="Source Files"
			Filters="*.java;*.c;*.cpp;*.cxx;*.hxx;*.prg;*.pas;*.dpr;*.asm;*.bas;*.sc;*.e"/>
		<Folder
			Name="Ant XML Build Files"
			Filters="*.xml"/>
		<Folder
			Name="Bitmaps"
			Filters="*.jpg;*.gif;*.bmp"/>
		<Folder
			Name="Other Files"
			Filters=""/>
	</CustomFolders>
	<Files AutoFolders="DirectoryView">
		<Folder Name="../pack">
			<Folder Name="eggdrop">
				<Folder Name="prolog">
					<F N="../pack/eggdrop/prolog/eggdrop.pl"/>
				</Folder>
			</Folder>
			<Folder Name="logicmoo_base">
				<Folder Name="prolog">
					<F
						N="../pack/logicmoo_base/prolog/*.*"
						Recurse="1"
						Refilter="1"
						Excludes=".git/;.svn/;*~;*.dll;*.class;*.jar;*.exe;*.o;*.a;*.zip;*.bak;*.*z"/>
				</Folder>
			</Folder>
		</Folder>
		<Folder Name="../../swipl-devel">
			<F
				N="../../swipl-devel/*.*"
				Recurse="1"
				Excludes=".git/;.svn/;pack/;*~;*.dll;*.class;*.jar;*.exe;*.o;*.a;*.zip;*.bak;*.*z"/>
		</Folder>
	</Files>
